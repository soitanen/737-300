      <equals>
    <property>/controls/autoflight/display-mach</property>>
    <value>false</value>
   </equals>
  </condition>
 </animation>
 
  <animation>
  <type>select</type>
  <object-name>machp</object-name>
  <object-name>mach1</object-name>
  <object-name>mach2</object-name>
