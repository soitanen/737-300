 <!-- AutoPilot Speed -->
 
 <animation>
  <type>textranslate</type>
  <object-name>mach2</object-name>
  <property>/autopilot/settings/target-mach</property>
  <factor>1.0</factor>
  <step>0.1</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 
<animation>
  <type>textranslate</type>
  <object-name>mach1</object-name>
