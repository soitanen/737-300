   <mode>in-transit</mode>
   <path>Sounds/flaps.wav</path>
   <property>/surface-positions/flap-pos-norm</property>
   <volume>
    <factor>0.45</factor>
   </volume>
  </flap>

  <wind>
   <name>wind</name>
   <mode>looped</mode>
   <path>Sounds/wind-8khz.wav</path>
   <property>/velocities/airspeed-kt</property>
   <volume>
    <property>/position/altitude-ft</property>
    <factor>-0.000010</factor>
    <offset>1.0</offset>
    <min>0.1</min>
    <max>1.0</max>
   </volume>
   <volume>
     <property>/velocities/airspeed-kt</property>
     <factor>0.0010</factor>
     <min>0.03</min>
     <max>0.25</max>
   </volume>
   <pitch>
    <property>/velocities/airspeed-kt</property>
    <factor>0.01</factor>
    <offset>1.25</offset>
   </pitch>
  </wind>
 
  <stall>
   <name>stall</name>
	 <mode>looped</mode>
   <path>Aircraft/737-300/Sounds/stall.wav</path>
   <condition>
     <and>
       <greater-than>
         <property>/fdm/jsbsim/aero/alpha-deg</property>
	       <value>12.0</value>
	     </greater-than>
	     <greater-than>
	       <property>/velocities/airspeed-kt</property>
	       <value>20</value>
	     </greater-than>
	   </and>
	 </condition>
   <volume>
	   <factor>1</factor>
   </volume>
  </stall>

